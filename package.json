  "devDependencies": {
    "markdownlint-cli2": "0.18.1"
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "keywords": [
