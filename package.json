{
  "name": "tracks-monorepo",
  "version": "0.1.0",
  "description": "A Rails-like web framework for Go that generates idiomatic, production-ready applications",
  "private": true,
  "scripts": {
    "lint:md": "markdownlint-cli2 '**/*.md' '#node_modules' '#.chglog' '#website/node_modules'",
    "lint:md:fix": "markdownlint-cli2 --fix '**/*.md' '#node_modules' '#.chglog' '#website/node_modules'",
    "website:dev": "pnpm --filter website dev",
